{
	"headers": [
	  {
		"source": "/api/:path*",
		"destination": "/api/:path*",
		"headers": [
		  { "key": "Access-Control-Allow-Origin", "value": "*" },
		  { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PATCH,PUT,DELETE,OPTIONS" },
		  { "key": "Access-Control-Allow-Headers", "value": "Content-Type,Authorization" }
		]
	  }
	]
  }
  